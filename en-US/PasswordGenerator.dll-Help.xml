      <command:verb>Get</command:verb>
      <command:noun>Password</command:noun>
      <maml:description>
        <maml:para>{{ A port of the .NET Standard library "Password Generator" which generates random passwords with different settings to meet the OWASP requirements  }}</maml:para>
      </maml:description>
    </command:details>
