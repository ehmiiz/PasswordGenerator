    <command:examples>
      <command:example>
        <maml:title>-------------------------- Example 1 --------------------------</maml:title>
        <dev:code>PS C:\&gt; {{ Get-Password }}</dev:code>
        <dev:remarks>
          <maml:para>{{ Will return a random password with the default settings, 16 length }}</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- Example 2 --------------------------</maml:title>
        <dev:code>PS C:\&gt; {{ Get-Password -Length 32 }}</dev:code>
        <dev:remarks>
          <maml:para>{{ Will return a password which is 32 characters long }}</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- Example 3 --------------------------</maml:title>
        <dev:code>PS C:\&gt; {{ Get-Password -IncludeLowercase -IncludeUppercase -Length 21 }}</dev:code>
        <dev:remarks>
          <maml:para>{{ Will return a password which only contains lowercase and uppercase characters and is 21 characters long }}</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- Example 4 --------------------------</maml:title>
        <dev:code>PS C:\&gt; {{ Get-Password -IncludeNumeric }}</dev:code>
        <dev:remarks>
          <maml:para>{{ This will return a password which is just numbers and has a default length of 16 }}</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- Example 5 --------------------------</maml:title>
        <dev:code>PS C:\&gt; {{ Get-Password -IncludeLowercase -IncludeUppercase -IncludeSpecial }}</dev:code>
        <dev:remarks>
          <maml:para>{{ As above, here is how to get lower, upper and special characters using this approach }}</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- Example 6 --------------------------</maml:title>
        <dev:code>PS C:\&gt; {{ Get-Password -IncludeLowercase -IncludeUppercase -IncludeSpecial -Length 128 }}</dev:code>
        <dev:remarks>
          <maml:para>{{ This is the same as the above, but with a length of 128 }}</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- Example 7 --------------------------</maml:title>
        <dev:code>PS C:\&gt; {{ Get-Password -IncludeNumeric -Length 4 }}</dev:code>
        <dev:remarks>
          <maml:para>{{ Returns a 4 digit number, used for PIN or One TIme Passwords }}</maml:para>
        </dev:remarks>
      </command:example>
